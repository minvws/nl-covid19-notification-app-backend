<?xml version="1.0" encoding="utf-8"?>
<configuration>

	<system.web>
		<httpRuntime enableVersionHeader="false" />
	</system.web>
	<system.webServer>
		<httpProtocol>
			<customHeaders>
				<remove name="X-Powered-By" />
			</customHeaders>
		</httpProtocol>
		<security>
			<requestFiltering removeServerHeader="true" />
		</security>
	</system.webServer>

</configuration>
